{
  "name": "v-agile",
  "version": "0.0.1",
  "description": "A easy to use carousel component for Vue",
  "scripts": {
    "dev": "vite",
    "build:demo": "NODE_ENV=production vite build --mode demo",
    "build:lib": "NODE_ENV=production vite build --mode lib",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1",
    "release": "node scripts/release.js",
    "serve": "vite preview"
  },
  "main": "src/index.js",
  "unpkg": "lib/v-agile.umd.js",
  "files": [
    "lib/*",
    "src/*",
    "*.json",
    "*.js"
  ],
  "dependencies": {},
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.0.1",
    "@vue/compiler-sfc": "^3.2.27",
    "chalk": "^5.0.0",
    "conventional-changelog-cli": "^2.2.2",
    "enquirer": "^2.3.6",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^8.3.0",
    "execa": "^6.0.0",
    "lint-staged": "^12.2.2",
    "minimist": "^1.2.5",
    "prettier": "^2.5.1",
    "sass": "^1.49.0",
    "semver": "^7.3.5",
    "vite": "^2.7.13",
    "vue": "^3.2.27"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jambonn/v-agile.git"
  },
  "bugs": {
    "url": "https://github.com/jambonn/v-agile/issues"
  },
  "keywords": [
    "vue",
    "carousel",
    "slider",
    "responsive",
    "agile"
  ],
  "author": "Jambon <hoa.nx.dev@gmail.com>",
  "license": "MIT",
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --quiet"
    ],
    "*.ts?(x)": [
      "eslint --quiet"
    ]
  },
  "engines": {
    "node": ">=12.22.0"
  }
}
